@563196 = ~No, por favor, no más dolor...~
@563198 = ~Por favor, ¡salvadme!~
@563199 = ~No me llevéis más a una batalla, maestro...~
@563200 = ~Liberadme, liberadme, ¡por favor!~
@563201 = ~No dejéis que me hieran...~
@563202 = ~Salieron, salieron de alguna parte... Ahora estoy aquí... Nadie vio que me hicieron...~
@563203 = ~No puedo soportar el dolor...~
@563204 = ~Lo hizo. Nunca pensé que realmente lo haría, nunca.~
@563205 = ~Por favor, guiadme a mi descanso...~
@563206 = ~¡Destruidme, destruidme! ¡Debéis hacerlo!~
@563207 = ~¡Aaaargh! Dejad... de... golpearme...~
@563208 = ~¡Maestro, matadlo, matadlo!~
@563209 = ~No, no más golpes, no más...~
@563210 = ~¡Aaaaargh, el dolor!~
@563211 = ~¡AAAAAAH! Matad a mi atacante...~
@563212 = ~No lo puedo soportar más, por favor...~
@563213 = ~¡No más, no más! ¡AAAARGH!~
@563214 = ~¡Matad, matad rápido!~
@563215 = ~¡AAAAAAAAAAAAAAAAAARRRGH!~
@563216 = ~¡NO! ¡NO! Rápido, poned fin a la batalla...~

@566966 = ~Extraéis el pergamino del cuerpo del hombre. Os indica la localización del hijo de Clob.~

@569140 = ~¡Escapad! ¡Bebed las pociones!~
@569141 = ~¡No tengo ninguna!~

@569152 = ~Tenéis... ¡Ey! ¡Alguien viene!~
@569153 = ~... Los conozco. Están aquí para capturarme.~
@569154 = ~Bueno, entonces... ¡Porqué no les enseñamos una lección!~

@571165 = ~El hermano Lucas murió. Habéis fallado en protegerlo.~
@571166 = ~(Esa debe haber sido Deneir...)~
@573572 = ~El Símbolo vibra.~

@571351 = ~¿¡Qué!?´¿A dónde se fue mi espada?~
@571352 = ~Aquí está, Olga.~
@571353 = ~¡Sucio ladrón!~
@571356 = ~¡Uups!~
@571357 = ~¡Comida!~
@571361 = ~Mmm... ¡Sabroso!~
@571359 = ~(¡Otra vez no!) Jenave, Alzad a los Muertos...~
@571365 = ~¿¡QUEEE!? ¡Mi comida ha vuelto a la vida!~
@571362 = ~Creo que ahora será mejor marcharnos...~

@573848 = ~Un frío viento sale del lago, escalofriando vuestros huesos. No es sabio dormir fuera.~







